package ej3;

public class Coche {
    private String marca;
    private String modelo;

    public Coche(String marca, String modelo) {
        this.marca = marca;
        this.modelo = modelo;
    }

    public String getMarca() {
        return marca;
    }

    public String getModelo() {
        return modelo;
    }

    public String mostrarDetalleDisponibilidad(Disponibilidad disponibilidad) {
        return "El coche " + marca + " " + modelo + " tiene una " + disponibilidad;
    }
}